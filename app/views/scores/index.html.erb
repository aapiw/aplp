<p id="notice"><%= notice %></p>

<h1>Scores</h1>

<table>
  <thead>
    <tr>
      <th>Plot</th>
      <th>Content</th>
      <th>Bahasa</th>
      <th>Fluency</th>
      <th>Interlude</th>
      <th>Gesture</th>
      <th>Sound</th>
      <th>Duration</th>
      <th>Showing</th>
      <th>Type</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scores.each do |score| %>
      <tr>
        <td><%= score.plot %></td>
        <td><%= score.content %></td>
        <td><%= score.bahasa %></td>
        <td><%= score.fluency %></td>
        <td><%= score.interlude %></td>
        <td><%= score.gesture %></td>
        <td><%= score.sound %></td>
        <td><%= score.duration %></td>
        <td><%= score.showing %></td>
        <td><%= score.type %></td>
        <td><%= score.user %></td>
        <td><%= link_to 'Show', score %></td>
        <td><%= link_to 'Edit', edit_score_path(score) %></td>
        <td><%= link_to 'Destroy', score, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Score', new_score_path %>
