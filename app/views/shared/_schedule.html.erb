            <%= I18n.l(schedule.start_consulate_selection , format: :long)%>
            <%#= @schedule_view.start_consulate_selection %>
          </td>
        </tr>
        <tr>
          <td>Selesai seleksi KBRI/KJRI</td>
          <td>
            <%= I18n.l(schedule.end_consulate_selection , format: :long)%>
            <%#= @schedule_view.end_consulate_selection %>
          </td>
        </tr>
        <tr>
          <td>Mulai seleksi Pusat</td>
          <td>
            <%= I18n.l(schedule.start_central_selection, format: :long)%>
            <%#= @schedule_view.start_central_selection %>
          </td>
        </tr>
        <tr>
          <td>Selesai seleksi Pusat (pengumuman)</td>
          <td>
            <%= I18n.l(schedule.end_central_selection, format: :long)%>
            <%#= @schedule_view.end_central_selection %>
          </td>
        </tr>
      </table>
    </div>
  </div>
</div>