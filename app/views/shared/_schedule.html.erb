<div class="card">
  <div class="header">
    <h2>Jadwal</h2>
  </div>
  <div class="body">
    <div class="table-responsive">
      <table class="table table-hover table-striped dashboard-task-infos">
        <tr>
          <td>Mulai registrasi</td>
          <td>
            <%= I18n.l(schedule.start_registration, format: :long)%>
            <%#= @schedule_view.start_registration %>
          </td>
        </tr>
        <tr>
          <td>Selesai registrasi</td>
          <td>
            <%= I18n.l(schedule.end_registration, format: :long)%>
            <%#= @schedule_view.end_registration %>
          </td>
