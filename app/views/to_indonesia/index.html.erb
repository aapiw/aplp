<p id="notice"><%= notice %></p>

<h1>To Indonesia</h1>

<table>
  <thead>
    <tr>
      <th>Destination</th>
      <th>Long</th>
      <th>Unit</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @to_indonesia.each do |to_indonesium| %>
      <tr>
        <td><%= to_indonesium.destination %></td>
        <td><%= to_indonesium.long %></td>
        <td><%= to_indonesium.unit %></td>
        <td><%= to_indonesium.user %></td>
        <td><%= link_to 'Show', to_indonesium %></td>
        <td><%= link_to 'Edit', edit_to_indonesium_path(to_indonesium) %></td>
        <td><%= link_to 'Destroy', to_indonesium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New To Indonesium', new_to_indonesium_path %>
