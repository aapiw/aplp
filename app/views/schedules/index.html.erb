<div class="row clearfix">
    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
        <div class="card">
            <div class="header">
                <h2>
                    EDIT JADWAL
                </h2>
            </div>
            <div class="body">
              <h2 class="card-inside-title">Mulai registrasi</h2>
              <%= form_for(@schedule) do |f| %>
                <div class="form-group form-float">
                    <div class="form-line">
                        <%= f.text_field :start_registration, value:I18n.l(@schedule.start_registration, format: :long), class: "form-control datepicker"%>
                    </div>
                </div>
                <h2 class="card-inside-title">Selesai registrasi</h2>
                <div class="form-group form-float">
                    <div class="form-line">
                        <%= f.text_field :end_registration, value:I18n.l(@schedule.end_registration, format: :long), class: "form-control datepicker"%>
                        
                    </div>
                </div>
                <h2 class="card-inside-title">Mulai seleksi KBRI/KJRI</h2>
                <div class="form-group form-float">
                    <div class="form-line">
                        <%= f.text_field :start_consulate_selection,value:I18n.l(@schedule.start_consulate_selection , format: :long), class: "form-control datepicker"%>
                        
                    </div>
                </div>
                <h2 class="card-inside-title">Selesai seleksi KBRI/KJRI</h2>
                <div class="form-group form-float">
                    <div class="form-line">
                        <%= f.text_field :end_consulate_selection, value:I18n.l(@schedule.end_consulate_selection , format: :long), class: "form-control datepicker"%>
                        
                    </div>
                </div>
                <h2 class="card-inside-title">Mulai seleksi Pusat</h2>
                <div class="form-group form-float">
                    <div class="form-line">
                        <%= f.text_field :start_central_selection , value:I18n.l(@schedule.start_central_selection, format: :long), class: "form-control datepicker"%>
                        
                    </div>
                </div>
                <h2 class="card-inside-title">Selesai seleksi Pusat (pengumuman)</h2>
                <div class="form-group form-float">
                    <div class="form-line">
                        <%= f.text_field :end_central_selection, value:I18n.l(@schedule.end_central_selection, format: :long), class: "form-control datepicker"%>
                        
                    </div>
                </div>
              <%= f.submit "SIMPAN", class: "btn btn-primary waves-effect"%>
              <!-- <button class="btn btn-primary waves-effect" type="submit">SIMPAN</button> -->
            <% end %>
          </div>
        </div>
    </div>
    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
      <%= render "shared/schedule" %>
   </div>



</div><!-- ROW -->