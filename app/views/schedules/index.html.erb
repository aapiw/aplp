<p id="notice"><%= notice %></p>

<h1>Schedules</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Start registration</th>
      <th>End registration</th>
      <th>Start central selection</th>
      <th>Start consulate selection</th>
      <th>End consulate selection</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedules.each do |schedule| %>
      <tr>
        <td><%= schedule.year %></td>
        <td><%= schedule.start_registration %></td>
        <td><%= schedule.end_registration %></td>
        <td><%= schedule.start_central_selection %></td>
        <td><%= schedule.start_consulate_selection %></td>
        <td><%= schedule.end_consulate_selection %></td>
        <td><%= link_to 'Show', schedule %></td>
        <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
        <td><%= link_to 'Destroy', schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schedule', new_schedule_path %>
